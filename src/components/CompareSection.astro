---
import ImagenHoverText from "./ImagenHoverText.astro";
export interface Props {

    title: string;
}
const { title } = Astro.props;
---
<div class="section">
    <div class="container">
        <h1 class='title-text blue'>{title}</h1>
        <slot />
    </div>
</div>
<hr class="blue-background">

<style>
    hr{
        width: 80%;
        height: 1px;
        margin: auto;
    }

</style>